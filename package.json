{
  "name": "neoconnect-chat",
  "version": "1.0.0",
  "description": "Modern chat application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build-exe": "pkg . --target node18-win-x64 --output neoconnect.exe",
    "build-linux": "pkg . --target node18-linux-x64 --output neoconnect-linux",
    "build-mac": "pkg . --target node18-macos-x64 --output neoconnect-mac",
    "build-all": "pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --output neoconnect"
  },
  "bin": "server.js",
  "pkg": {
    "assets": [
      "public/**/*"
    ],
    "scripts": []
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.5.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
